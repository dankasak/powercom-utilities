create or replace view vw_shoulder as
select
    id
  , reading_datetime
  , reading_type
  , case
        when ( reading_datetime::TIME between '21:00:00' and '21:59:59'
            or reading_datetime::TIME between '07:00:00' and '14:59:59'
        ) then watts
          else 0
    end as watts
from
    public.powercom_readings
where
    reading_type = 'Consumption'
